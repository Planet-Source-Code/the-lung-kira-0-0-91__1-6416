        Case WSA_QOS_ESDMODEOBJ: errDescription = "An invalid shape discard mode object was found in the QOS provider-specific buffer.": errConst = "WSA_QOS_ESDMODEOBJ"
        Case WSA_QOS_ESHAPERATEOBJ: errDescription = "An invalid shaping rate object was found in the QOS provider-specific buffer.": errConst = "WSA_QOS_ESHAPERATEOBJ"
        Case WSA_QOS_RESERVED_PETYPE: errDescription = "A reserved policy element was found in the QOS provider-specific buffer.": errConst = "WSA_QOS_RESERVED_PETYPE"
    End Select
    
    'Do not send blank message
    If NoMsgBox = False Then
        If errMsg = True Then
            MsgBox apiFunction & vbCrLf & vbCrLf & errConst & vbCrLf & vbCrLf & errDescription, vbExclamation, "Error"
        End If
    End If
End Function

Public Function Failed(strAPI As String)
    If errMsg = True Then
        MsgBox strAPI & vbCrLf & vbCrLf & "Failed", vbExclamation, "Error"
    End If
End Function
